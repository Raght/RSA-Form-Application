<Window x:Class="RSA.PickPrimesWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RSA"
        mc:Ignorable="d"
        Title="Введите простые числа" Height="154" Width="236">
    <Grid>
        <Label Content="p:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <Label Content="q:" HorizontalAlignment="Left" Margin="114,10,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="TextBoxFirstPrime" HorizontalAlignment="Left" Margin="30,14,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="64" TextChanged="TextBoxFirstPrime_TextChanged"/>
        <TextBox x:Name="TextBoxSecondPrime" HorizontalAlignment="Left" Margin="134,14,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="64" TextChanged="TextBoxSecondPrime_TextChanged"/>
        <Button x:Name="EnterExample" Content="Пример" HorizontalAlignment="Left" Margin="10,47,0,0" VerticalAlignment="Top" Click="EnterExample_Click"/>
        <Button x:Name="EnterPrimes" Content="Ввести числа" HorizontalAlignment="Left" Margin="10,83,0,0" VerticalAlignment="Top" Click="EnterPrimes_Click"/>
    </Grid>
</Window>
